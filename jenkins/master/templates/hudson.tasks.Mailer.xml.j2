<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl plugin="mailer@1.20">
  <defaultSuffix>{{ jenkins.email.default_email_suffix }}</defaultSuffix>
  <smtpAuthUsername>{{ jenkins.email.smtp_user }}</smtpAuthUsername>
  <smtpAuthPassword>{{ jenkins.email.smtp_passwd }}</smtpAuthPassword>
  <smtpHost>{{ jenkins.email.smtp_host }}</smtpHost>
  <smtpPort>{{ jenkins.email.smtp_port }}</smtpPort>
  <useSsl>{{ jenkins.email.smtp_ssl }}</useSsl>
  <charset>UTF-8</charset>
</hudson.tasks.Mailer_-DescriptorImpl>
